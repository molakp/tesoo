<Mine rdf:about="#Del's_Claim">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  <is_located rdf:resource="#Auridon"/>
  <dungeon_completition_type>Explore and Clear</dungeon_completition_type>
