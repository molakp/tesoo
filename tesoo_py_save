    <rdf:Description>
      <rdf:first rdf:resource="#npc_killed_to_complete_dungeon"/>
      <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
    </rdf:Description>
  </owl:propertyChainAxiom>
  <owl:propertyChainAxiom>
    <rdf:Description>
      <rdf:first rdf:resource="#npc_located_in"/>
      <rdf:rest>
        <rdf:Description>
          <rdf:first rdf:resource="#is_located"/>
          <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
        </rdf:Description>
      </rdf:rest>
    </rdf:Description>
  </owl:propertyChainAxiom>
</owl:ObjectProperty>

