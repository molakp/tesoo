<owl:ObjectProperty rdf:about="#belongs_to_organization">
  <owl:inverseOf rdf:resource="#organization_has_member"/>
  <rdfs:domain>
    <owl:Class>
      <owl:unionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#character"/>
        <rdf:Description rdf:about="#npc"/>
      </owl:unionOf>
    </owl:Class>
  </rdfs:domain>
  <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
</owl:ObjectProperty>

