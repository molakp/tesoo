<owl:Class rdf:about="#npc">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2000/10/swap/pim/contact#foaf:Person"/>
  <owl:disjointUnionOf>
    <rdf:Description>
      <rdf:first rdf:resource="#Friendly"/>
      <rdf:rest>
        <rdf:Description>
          <rdf:first rdf:resource="#Hostile"/>
          <rdf:rest>
            <rdf:Description>
              <rdf:first rdf:resource="#Justice_Neutral"/>
              <rdf:rest>
                <rdf:Description>
                  <rdf:first rdf:resource="#Neutral"/>
                  <rdf:rest>
                    <rdf:Description>
                      <rdf:first rdf:resource="#None"/>
                      <rdf:rest>
                        <rdf:Description>
                          <rdf:first rdf:resource="#Vary"/>
                          <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                        </rdf:Description>
                      </rdf:rest>
                    </rdf:Description>
                  </rdf:rest>
                </rdf:Description>
