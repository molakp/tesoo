@prefix : <http://www.semanticweb.org/TESOO> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix vgo: <http://purl.org/net/videogameontology#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/spec/index.rdf> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix swrl: <http://www.w3.org/2003/11/swrl#> .
@prefix vann: <http://www.semanticweb.org/TESOO#vann:> .
@prefix swrla: <http://swrl.stanford.edu/ontologies/3.3/swrla.owl#> .
@prefix swrlb: <http://www.w3.org/2003/11/swrlb#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix tesoo: <http://www.semanticweb.org/TESOO#> .
@prefix schema: <http://schema.org/> .
@base <http://www.semanticweb.org/TESOO> .

<http://www.semanticweb.org/TESOO> rdf:type owl:Ontology ;
                                    dc:creator "Silvestro Stefano Frisullo"@en ;
                                    dc:date "June 2022"@en ;
                                    dc:description "Ontology for The Elder Scrolls Online"@en ;
                                    dc:title "TESOO: The Elder Scrolls Online Ontology"@en ;
                                    terms:license "http://opensource.org/licenses/mit-license.php/" ;
                                    vann:preferredNamespacePrefix "tesoo"@en ;
                                    vann:preferredNamespaceUri "http://www.tesoo.com"@en ;
                                    rdfs:label "TESOO"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
dc:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
dc:date rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
dc:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
dc:title rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/bibliographicCitation
terms:bibliographicCitation rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
terms:license rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/source
terms:source rdf:type owl:AnnotationProperty .


###  http://schema.org/image
schema:image rdf:type owl:AnnotationProperty .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
swrla:isRuleEnabled rdf:type owl:AnnotationProperty .


###  http://www.semanticweb.org/TESOO#definition
tesoo:definition rdf:type owl:AnnotationProperty .


###  http://www.semanticweb.org/TESOO#vann:preferredNamespacePrefix
vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://www.semanticweb.org/TESOO#vann:preferredNamespaceUri
vann:preferredNamespaceUri rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/TESOO#belongs_to_organization
tesoo:belongs_to_organization rdf:type owl:ObjectProperty ;
                              owl:inverseOf tesoo:organization_has_member ;
                              rdf:type owl:FunctionalProperty ,
                                       owl:AsymmetricProperty ,
                                       owl:IrreflexiveProperty ;
                              rdfs:domain [ rdf:type owl:Class ;
                                            owl:unionOf ( tesoo:Character
                                                          tesoo:Npc
                                                          tesoo:Race
                                                        )
                                          ] ;
                              rdfs:range <http://xmlns.com/foaf/0.1/Organization> ;
                              rdfs:comment <http://www.semanticweb.org/belongs_to_organization> ;
                              rdfs:label "belongs to organization"@en .


###  http://www.semanticweb.org/TESOO#belongs_to_race
tesoo:belongs_to_race rdf:type owl:ObjectProperty ;
                      owl:inverseOf tesoo:race_has_member ;
                      rdf:type owl:FunctionalProperty ,
                               owl:AsymmetricProperty ,
                               owl:IrreflexiveProperty ;
                      rdfs:domain [ rdf:type owl:Class ;
                                    owl:unionOf ( tesoo:Character
                                                  tesoo:Npc
                                                )
                                  ] ;
                      rdfs:range tesoo:Race ;
                      rdfs:comment "What race an individual belongs to"@en ;
                      rdfs:label "belongs to race"@en .


###  http://www.semanticweb.org/TESOO#character_is_attacked
tesoo:character_is_attacked rdf:type owl:ObjectProperty ;
                            owl:inverseOf tesoo:npc_will_attack_character ;
                            rdfs:domain tesoo:Character ;
                            rdfs:range tesoo:Npc ;
                            rdfs:comment "Tells if character is attacked by a npc"@en ;
                            rdfs:label "character is attacked" .


###  http://www.semanticweb.org/TESOO#city_ruled_by
tesoo:city_ruled_by rdf:type owl:ObjectProperty ;
                    owl:inverseOf tesoo:npc_rules_city ;
                    rdfs:domain tesoo:City ;
                    rdfs:range tesoo:Npc ;
                    rdfs:comment "Tells by what NPC the city is ruled"@en ;
                    rdfs:label "city ruled by" .


###  http://www.semanticweb.org/TESOO#contains_place
tesoo:contains_place rdf:type owl:ObjectProperty ;
                     owl:inverseOf tesoo:is_located ;
                     rdfs:domain tesoo:zone ;
                     rdfs:range tesoo:Specific_Place ;
                     rdfs:comment "Connect a zone to a place that it contains"@en ;
                     rdfs:label "contains place" .


###  http://www.semanticweb.org/TESOO#dungeon_kill_to_complete
tesoo:dungeon_kill_to_complete rdf:type owl:ObjectProperty ;
                               owl:inverseOf tesoo:npc_killed_to_complete_dungeon ;
                               rdfs:domain tesoo:Specific_Place ;
                               rdfs:range tesoo:Npc ;
                               rdfs:comment "Usually in order to complete a dungeon the character must defeat the boss, this relation links each dungeon to its final boss."@en ;
                               rdfs:label "dungeon kill to complete" .


###  http://www.semanticweb.org/TESOO#faction_controls
tesoo:faction_controls rdf:type owl:ObjectProperty ;
                       owl:inverseOf tesoo:zone_controlled_by ;
                       rdf:type owl:AsymmetricProperty ;
                       rdfs:domain tesoo:Faction ;
                       rdfs:range tesoo:zone ;
                       rdfs:comment "A faction controls a zone"@en ;
                       rdfs:label "faction controls" .


###  http://www.semanticweb.org/TESOO#has_first_quest
tesoo:has_first_quest rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf owl:topObjectProperty ;
                      owl:inverseOf tesoo:is_first_quest_of ;
                      rdfs:domain <http://ontologydesignpatterns.org/wiki/Submissions:List#QuestLine> ;
                      rdfs:range tesoo:Quest ;
                      rdfs:comment "The first quest of a questline"@en ;
                      rdfs:label "has first quest" .


###  http://www.semanticweb.org/TESOO#has_last_quest
tesoo:has_last_quest rdf:type owl:ObjectProperty ;
                     owl:inverseOf tesoo:is_last_quest_of ;
                     rdf:type owl:FunctionalProperty ;
                     rdfs:domain <http://ontologydesignpatterns.org/wiki/Submissions:List#QuestLine> ;
                     rdfs:range tesoo:Quest ;
                     rdfs:comment "The last quest of a questline"@en ;
                     rdfs:label "has last quest" .


###  http://www.semanticweb.org/TESOO#has_less_health
tesoo:has_less_health rdf:type owl:ObjectProperty ;
                      owl:inverseOf tesoo:has_more_health ;
                      rdfs:domain [ rdf:type owl:Class ;
                                    owl:unionOf ( tesoo:Character
                                                  tesoo:Npc
                                                )
                                  ] ;
                      rdfs:range [ rdf:type owl:Class ;
                                   owl:unionOf ( tesoo:Character
                                                 tesoo:Npc
                                               )
                                 ] ;
                      rdfs:comment "inverse of has more health"@en ;
                      rdfs:label "has less health" .


###  http://www.semanticweb.org/TESOO#has_more_health
tesoo:has_more_health rdf:type owl:ObjectProperty ,
                               owl:AsymmetricProperty ,
                               owl:IrreflexiveProperty ;
                      rdfs:domain [ rdf:type owl:Class ;
                                    owl:unionOf ( tesoo:Character
                                                  tesoo:Npc
                                                )
                                  ] ;
                      rdfs:range [ rdf:type owl:Class ;
                                   owl:unionOf ( tesoo:Character
                                                 tesoo:Npc
                                               )
                                 ] ;
                      rdfs:comment "Which NPC the characet has more health than."@en ;
                      rdfs:label "has more health" .


###  http://www.semanticweb.org/TESOO#has_next_quest
tesoo:has_next_quest rdf:type owl:ObjectProperty ;
                     owl:inverseOf tesoo:has_prerequisite_quest ;
                     rdfs:domain tesoo:Quest ;
                     rdfs:range tesoo:Quest ;
                     rdfs:comment "link a quest to the next"@en ;
                     rdfs:label "has next quest" .


###  http://www.semanticweb.org/TESOO#has_prerequisite_quest
tesoo:has_prerequisite_quest rdf:type owl:ObjectProperty ;
                             rdfs:domain tesoo:Quest ;
                             rdfs:range tesoo:Quest ;
                             rdfs:comment "links a quest to its prerequisite"@en ;
                             rdfs:label "has prerequisite quest" .


###  http://www.semanticweb.org/TESOO#has_quest_giver
tesoo:has_quest_giver rdf:type owl:ObjectProperty ;
                      owl:inverseOf tesoo:npc_is_quest_giver ;
                      rdf:type owl:AsymmetricProperty ;
                      rdfs:domain tesoo:Quest ;
                      rdfs:range tesoo:Npc ;
                      rdfs:comment "tells which NPC is the giver of the quest"@en ;
                      rdfs:label "has quest giver" .


###  http://www.semanticweb.org/TESOO#is_first_quest_of
tesoo:is_first_quest_of rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf tesoo:quest_is_in_questline ;
                        rdf:type owl:FunctionalProperty ;
                        rdfs:domain tesoo:Quest ;
                        rdfs:range <http://ontologydesignpatterns.org/wiki/Submissions:List#QuestLine> ;
                        rdfs:comment "Assert that a specific quest is the first in a questline." ;
                        rdfs:label "is first quest of" .


###  http://www.semanticweb.org/TESOO#is_last_quest_of
tesoo:is_last_quest_of rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf tesoo:quest_is_in_questline ;
                       rdfs:domain tesoo:Quest ;
                       rdfs:range <http://ontologydesignpatterns.org/wiki/Submissions:List#QuestLine> ;
                       rdfs:comment "Assert that a quest is the last of a questline"@en ;
                       rdfs:label "is last quest of" .


###  http://www.semanticweb.org/TESOO#is_located
tesoo:is_located rdf:type owl:ObjectProperty ;
                 rdfs:domain tesoo:Specific_Place ;
                 rdfs:range tesoo:zone ;
                 rdfs:comment "locates a specific place into a Zone"@en ;
                 rdfs:label "is located" .


###  http://www.semanticweb.org/TESOO#is_location_of_npc
tesoo:is_location_of_npc rdf:type owl:ObjectProperty ;
                         owl:inverseOf tesoo:npc_located_in ;
                         rdfs:domain <http://purl.org/net/VideoGameOntology#PlayingArea> ;
                         rdfs:range tesoo:Npc ;
                         rdfs:comment "tells that a specific place is the location of a npc"@en ;
                         rdfs:label "is location of npc" .


###  http://www.semanticweb.org/TESOO#is_location_of_quest
tesoo:is_location_of_quest rdf:type owl:ObjectProperty ;
                           owl:inverseOf tesoo:quest_located_in ;
                           rdfs:domain <http://purl.org/net/VideoGameOntology#PlayingArea> ;
                           rdfs:range tesoo:Quest ;
                           rdfs:comment "tells that a playing area is the location of a quest"@en ;
                           rdfs:label "is location of quest" .


###  http://www.semanticweb.org/TESOO#npc_belongs_same_organization
tesoo:npc_belongs_same_organization rdf:type owl:ObjectProperty ,
                                             owl:SymmetricProperty ,
                                             owl:TransitiveProperty ;
                                    rdfs:domain tesoo:Npc ;
                                    rdfs:range tesoo:Npc ;
                                    rdfs:comment "Used to asset that two npc are in same organization" ;
                                    rdfs:label "npc belongs same organization" .


###  http://www.semanticweb.org/TESOO#npc_in_relationship_with
tesoo:npc_in_relationship_with rdf:type owl:ObjectProperty ,
                                        owl:SymmetricProperty ;
                               rdfs:domain tesoo:Npc ;
                               rdfs:range tesoo:Npc ;
                               rdfs:comment "Links two npc that are in a general relationship"@en ;
                               rdfs:label "npc in relationship with" .


###  http://www.semanticweb.org/TESOO#npc_is_attacked
tesoo:npc_is_attacked rdf:type owl:ObjectProperty ;
                      rdfs:domain tesoo:Npc ;
                      rdfs:range [ rdf:type owl:Class ;
                                   owl:unionOf ( tesoo:Character
                                                 tesoo:Npc
                                               )
                                 ] ;
                      rdfs:comment "tells that an npc is attacked by someone"@en ;
                      rdfs:label "npc is attacked" .


###  http://www.semanticweb.org/TESOO#npc_is_married_with
tesoo:npc_is_married_with rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf tesoo:npc_in_relationship_with ;
                          rdf:type owl:SymmetricProperty ;
                          rdfs:domain tesoo:Npc ;
                          rdfs:range tesoo:Npc ;
                          rdfs:comment "link two married npc"@en ;
                          rdfs:label "npc is married with" .


###  http://www.semanticweb.org/TESOO#npc_is_quest_giver
tesoo:npc_is_quest_giver rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf tesoo:npc_related_quest ;
                         rdfs:domain tesoo:Npc ;
                         rdfs:range tesoo:Quest ;
                         rdfs:comment "Tells taht an noc is the giver of a quest"@en ;
                         rdfs:label "npc is quest giver" .


###  http://www.semanticweb.org/TESOO#npc_killed_to_complete_dungeon
tesoo:npc_killed_to_complete_dungeon rdf:type owl:ObjectProperty ,
                                              owl:FunctionalProperty ;
                                     rdfs:domain tesoo:Npc ;
                                     rdfs:range tesoo:Specific_Place ;
                                     rdfs:comment "Tells that an NPC is the final boss of a dungeon"@en ;
                                     rdfs:label "npc killed to complete dungeon" .


###  http://www.semanticweb.org/TESOO#npc_located_in
tesoo:npc_located_in rdf:type owl:ObjectProperty ;
                     rdfs:domain tesoo:Npc ;
                     rdfs:range <http://purl.org/net/VideoGameOntology#PlayingArea> ;
                     rdfs:comment "Describe the location of an npc"@en ;
                     rdfs:label "npc located in" .


###  http://www.semanticweb.org/TESOO#npc_located_in_zone
tesoo:npc_located_in_zone rdf:type owl:ObjectProperty ;
                          owl:inverseOf tesoo:zone_is_location_of_npc ;
                          rdfs:domain tesoo:Npc ;
                          rdfs:range tesoo:zone ;
                          owl:propertyChainAxiom ( tesoo:npc_located_in
                                                   tesoo:is_located
                                                 ) ;
                          rdfs:comment "Property chain for the location of npc's in zone" ;
                          rdfs:label "npc located in zone" .


###  http://www.semanticweb.org/TESOO#npc_related_quest
tesoo:npc_related_quest rdf:type owl:ObjectProperty ;
                        owl:inverseOf tesoo:quest_related_to_npc ;
                        rdfs:domain tesoo:Npc ;
                        rdfs:range tesoo:Quest ;
                        rdfs:comment "tells what quest an npc is related to"@en ;
                        rdfs:label "npc related quest" .


###  http://www.semanticweb.org/TESOO#npc_rules_city
tesoo:npc_rules_city rdf:type owl:ObjectProperty ;
                     rdfs:domain tesoo:Npc ;
                     rdfs:range tesoo:City ;
                     rdfs:comment "Tells than an npc rule a city"@en ;
                     rdfs:label "npc rules city" .


###  http://www.semanticweb.org/TESOO#npc_will_attack
tesoo:npc_will_attack rdf:type owl:ObjectProperty ,
                               owl:IrreflexiveProperty ;
                      rdfs:domain tesoo:Npc ;
                      rdfs:range [ rdf:type owl:Class ;
                                   owl:unionOf ( tesoo:Character
                                                 tesoo:Npc
                                               )
                                 ] ;
                      rdfs:comment "Links an NPC to the other npc or characters that it will attack"@en ;
                      rdfs:label "npc will attack" .


###  http://www.semanticweb.org/TESOO#npc_will_attack_character
tesoo:npc_will_attack_character rdf:type owl:ObjectProperty ;
                                rdfs:subPropertyOf tesoo:npc_will_attack ;
                                rdfs:domain tesoo:Npc ;
                                rdfs:range tesoo:Character ;
                                rdfs:comment "a subproperty, tell that an npc will attack the character"@en ;
                                rdfs:label "npc will attack character" .


###  http://www.semanticweb.org/TESOO#organization_has_member
tesoo:organization_has_member rdf:type owl:ObjectProperty ;
                              rdfs:domain <http://xmlns.com/foaf/0.1/Organization> ;
                              rdfs:range [ rdf:type owl:Class ;
                                           owl:unionOf ( tesoo:Character
                                                         tesoo:Npc
                                                         tesoo:Race
                                                       )
                                         ] ;
                              rdfs:comment "connect an organization to its members"@en ;
                              rdfs:label "organization has member" .


###  http://www.semanticweb.org/TESOO#quest_is_in_questline
tesoo:quest_is_in_questline rdf:type owl:ObjectProperty ;
                            owl:inverseOf tesoo:questline_has_quest ;
                            rdf:type owl:FunctionalProperty ;
                            rdfs:domain tesoo:Quest ;
                            rdfs:range <http://ontologydesignpatterns.org/wiki/Submissions:List#QuestLine> ;
                            rdfs:comment "connect a quest to its questline"@en ;
                            rdfs:label "quest is in questline" .


###  http://www.semanticweb.org/TESOO#quest_located_in
tesoo:quest_located_in rdf:type owl:ObjectProperty ;
                       rdfs:domain tesoo:Quest ;
                       rdfs:range <http://purl.org/net/VideoGameOntology#PlayingArea> ;
                       owl:propertyChainAxiom ( tesoo:quest_located_in
                                                tesoo:is_located
                                              ) ;
                       rdfs:comment "Tell the location of a quest"@en ;
                       rdfs:label "quest located in" .


###  http://www.semanticweb.org/TESOO#quest_related_to_npc
tesoo:quest_related_to_npc rdf:type owl:ObjectProperty ;
                           rdfs:domain tesoo:Quest ;
                           rdfs:range tesoo:Npc ;
                           rdfs:comment "Link a quest to the npc's it involves"@en ;
                           rdfs:label "quest related to npc"@en .


###  http://www.semanticweb.org/TESOO#questline_has_quest
tesoo:questline_has_quest rdf:type owl:ObjectProperty ;
                          rdfs:domain <http://ontologydesignpatterns.org/wiki/Submissions:List#QuestLine> ;
                          rdfs:range tesoo:Quest ;
                          rdfs:comment "Used to link a questline to its quests"@en ;
                          rdfs:label "questline has quest"@en .


###  http://www.semanticweb.org/TESOO#race_has_member
tesoo:race_has_member rdf:type owl:ObjectProperty ;
                      rdfs:domain tesoo:Race ;
                      rdfs:range [ rdf:type owl:Class ;
                                   owl:unionOf ( tesoo:Character
                                                 tesoo:Npc
                                               )
                                 ] ;
                      rdfs:comment "Link a race to its individuals"@en ;
                      rdfs:label "race has member"@en .


###  http://www.semanticweb.org/TESOO#zone_controlled_by
tesoo:zone_controlled_by rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf owl:topObjectProperty ;
                         rdf:type owl:AsymmetricProperty ;
                         rdfs:domain tesoo:zone ;
                         rdfs:range tesoo:Faction ;
                         rdfs:comment "Asset that a zone is controlled by a faction"@en ;
                         rdfs:label "zone controlled by" .


###  http://www.semanticweb.org/TESOO#zone_is_location_of_npc
tesoo:zone_is_location_of_npc rdf:type owl:ObjectProperty ;
                              rdfs:subPropertyOf owl:topObjectProperty ;
                              rdfs:domain tesoo:zone ;
                              rdfs:range tesoo:Npc ;
                              rdfs:comment "Assert that a zone is the location of a npc"@en ;
                              rdfs:label "zone is location of npc" .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/TESOO#dungeon_completition_type
tesoo:dungeon_completition_type rdf:type owl:DatatypeProperty ;
                                rdfs:domain tesoo:Specific_Place ;
                                rdfs:range xsd:string ;
                                rdfs:comment "Dungeon can be completed in many ways, such as killing an npc, retrieving an object etc." ;
                                rdfs:label "dungeon completition type" .


###  http://www.semanticweb.org/TESOO#has_XP_Gain
tesoo:has_XP_Gain rdf:type owl:DatatypeProperty ;
                  rdfs:domain tesoo:Quest ;
                  rdfs:range xsd:string ;
                  rdfs:comment "XP gain of a quest"@en ;
                  rdfs:label "has xp gain" .


###  http://www.semanticweb.org/TESOO#has_gender
tesoo:has_gender rdf:type owl:DatatypeProperty ;
                 rdfs:domain <http://www.w3.org/2000/10/swap/pim/contact#foaf:Person> ;
                 rdfs:range xsd:string ;
                 rdfs:comment "gender of a person"@en ;
                 rdfs:label "has gender" .


###  http://www.semanticweb.org/TESOO#has_health
tesoo:has_health rdf:type owl:DatatypeProperty ;
                 rdfs:domain <http://www.w3.org/2000/10/swap/pim/contact#foaf:Person> ;
                 rdfs:range xsd:int ;
                 rdfs:comment "health of a person"@en ;
                 rdfs:label "has health" .


###  http://www.semanticweb.org/TESOO#has_name
tesoo:has_name rdf:type owl:DatatypeProperty ;
               rdfs:domain owl:Thing ;
               rdfs:range xsd:string ;
               rdfs:comment "name of something"@en ;
               rdfs:label "has name" .


###  http://www.semanticweb.org/TESOO#has_quest_id
tesoo:has_quest_id rdf:type owl:DatatypeProperty ;
                   rdfs:domain tesoo:Quest ;
                   rdfs:range xsd:int ;
                   rdfs:comment "the ID of a quest"@en ;
                   rdfs:label "has quest id" .


###  http://www.semanticweb.org/TESOO#has_race
tesoo:has_race rdf:type owl:DatatypeProperty ;
               rdfs:domain <http://www.w3.org/2000/10/swap/pim/contact#foaf:Person> ;
               rdfs:range xsd:string ;
               rdfs:comment "race of a Person"@en ;
               rdfs:label "has race" .


###  http://www.semanticweb.org/TESOO#has_reward
tesoo:has_reward rdf:type owl:DatatypeProperty ;
                 rdfs:domain tesoo:Quest ;
                 rdfs:range xsd:string ;
                 rdfs:comment "reward of a quest"@en ;
                 rdfs:label "has reward" .


###  http://www.semanticweb.org/TESOO#is_killable
tesoo:is_killable rdf:type owl:DatatypeProperty ;
                  rdfs:domain tesoo:Npc ;
                  rdfs:range xsd:boolean ;
                  rdfs:comment "specify if npc is killable by the player" ;
                  rdfs:label "is killable" .


###  http://www.semanticweb.org/TESOO#place_is_discoverable
tesoo:place_is_discoverable rdf:type owl:DatatypeProperty ;
                            rdfs:domain tesoo:Specific_Place ;
                            rdfs:range xsd:boolean ;
                            rdfs:comment "Some places are freely discoverable, other are locked untile specific actions are done or conditions are met"@en ;
                            rdfs:label "place is discoverable" .


###  http://www.semanticweb.org/TESOO#place_is_dungeon
tesoo:place_is_dungeon rdf:type owl:DatatypeProperty ;
                       rdfs:domain tesoo:Specific_Place ;
                       rdfs:range xsd:boolean ;
                       rdfs:comment "If a place is a dangeon" ;
                       rdfs:label "place is dungeon" .


###  http://www.semanticweb.org/TESOO#quest_solo_only
tesoo:quest_solo_only rdf:type owl:DatatypeProperty ;
                      rdfs:subPropertyOf owl:topDataProperty ;
                      rdfs:domain tesoo:Quest ;
                      rdfs:range xsd:boolean ;
                      rdfs:comment "true If a quest must be completed alone, false otherwise" ;
                      rdfs:label "quest solo only" .


#################################################################
#    Classes
#################################################################

###  http://ontologydesignpatterns.org/wiki/Submissions:List#QuestLine
<http://ontologydesignpatterns.org/wiki/Submissions:List#QuestLine> rdf:type owl:Class ;
                                                                    dc:description "A questline is a list of quest that have a specific chronological order"@en ;
                                                                    rdfs:comment "Implementation on ontology pattern List" ;
                                                                    rdfs:label "QuestLine" .


